dose  <- c(20, 30, 40, 45, 60)
drugA <- c(16, 20, 27, 40, 60)
drugB <- c(15, 18, 25, 31, 40)
##Type b indicates both Line and point
plot(dose, drugA, type="b")
##Type c only line 
plot(dose, drugA, type="c")
## Only Points
plot(dose,drugA)
## Triangle markers
plot(dose,drugA,type="b",pch=17,lty=2,col="red")
## Squre Markers
plot(dose, drugA, type="b", lty=3, lwd=3, pch=15, cex=2,col="blue")
